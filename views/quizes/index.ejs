<table>
  <tr><td>
  <form method="get" action="/quizes">
    Buscar Pregunta: <input type="text" name="search"/><input type="submit" value="Enviar"/>
  </form>
  </td></tr>
  <% var i; for (i=0; i < quizes.length; i++) { %>
    <tr>
      <td style="text-align:left;"><a href="/quizes/<%= quizes[i].id %>"><%= quizes[i].pregunta %></a></td>
      <td><a href="/quizes/<%= quizes[i].id %>/edit"><button>editar</button></a></td>
    </tr>
  <% } %>
</table>
<p><a href="/quizes/new"><button>Crear pregunta</button></a></p>
